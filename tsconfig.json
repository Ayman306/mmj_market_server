{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "pretty": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "rootDir": "./src"
      
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  